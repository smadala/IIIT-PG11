long getBufferSize(long fileSize){
long chunckSize;
if(fileSize < MIN_BUFFER)
return fileSize;
else if(fileSize <= 10*MIN_BUFFER)
return MIN_BUFFER;
else{
chunckSize=fileSize/10;
if(chunckSize > MAX_BUFFER)
return MAX_BUFFER;
return chunckSize;
