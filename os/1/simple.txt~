#include<stdio.h>
#include<stdlib.h>
#include<fcntl.h>
#include<unistd.h>
#include<sys/types.h>
#include<string.h>
#define MIN_BUFFER 1024 //1KB
#define MAX_BUFFER 10485760 //10MB
char* strrev(char *orginal) {
  long len=0,i,j;
  char *reverse;
  while(*orginal){
      orginal++;
      len++;
