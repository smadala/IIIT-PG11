  char *reverse;
  while(*orginal){
      orginal++;
      len++;
  }
  reverse=malloc((len+1)*sizeof(char));
  orginal=orginal-len; 
  for(i = 0, j = len-1; i < len ;i++,j--){
       *(reverse+i)=*(orginal+j);
  }
  return reverse;
