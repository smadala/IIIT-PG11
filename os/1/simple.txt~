#include<stdio.h>
#include<stdlib.h>
#include<fcntl.h>
#include<unistd.h>
#include<sys/types.h>
#include<string.h>
#define MIN_BUFFER 1024 //1KB
#define MAX_BUFFER 10485760 //10MB
char* strrev(char *orginal) {
  long len=0,i,j;
  char *reverse;
  while(*orginal){
      orginal++;
      len++;
  }
  reverse=malloc((len+1)*sizeof(char));
  orginal=orginal-len; 
  for(i = 0, j = len-1; i < len ;i++,j--){
       *(reverse+i)=*(orginal+j);
  }
  return reverse;
